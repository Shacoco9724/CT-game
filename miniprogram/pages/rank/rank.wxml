<!--pages/rank/rank.wxml-->
<text>pages/rank/rank.wxml</text>
<view>
    <text>冒泡排序排行榜</text>
    <view class="bubblesort_rank">
        <view class="table">
            <view class="tr bg-w">
                <view class="th">head1</view>
                <view class="th">head2</view>
                <view class="th ">head3</view>
            </view>
            <block wx:for="{{listdata}}" wx:key="{[openid]}">
            <view class="tr bg-g" wx:if="{{index % 2 == 0}}">
                <view class="td">{{item.openid}}</view>
                <view class="td">{{item.score}}</view>
                <!-- <view class="td">{{item.type}}</view> -->
            </view>
            <view class="tr" wx:else>
                <view class="td">{{item.openid}}</view>
                <view class="td">{{item.score}}</view>
                <!-- <view class="td">{{item.type}}</view> -->
            </view>
            </block>
        </view>
    </view>
</view>
<view class="search_rank">查找算法排行榜</view>
